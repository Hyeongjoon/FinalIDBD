<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.myapplication"
    android:versionCode="1"
    android:versionName="1.0"
    android:minSdkVersion="8"
    >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <service
        android:name=".MyFirebaseInstanceIDService">
        <intent-filter>
            <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
        </intent-filter>
    </service>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity_"/>
        <activity android:name=".SignUpActivity_"/>
        <activity android:name=".EmailSucActivity_"/>
        <activity android:name=".FindEmailActivity_"/>
        <activity android:name=".ModifyPwdActivity_"/>
        <activity android:name=".SaveActivity_"/>
        <activity android:name=".MainActivity_"/>
        <activity android:name=".Gr_info_Activity_"/>
        <activity android:name=".MyScheduleActivity_"/>
        <activity android:name=".IntroActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".Helper.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>